Задание 1:

public class Main
{
  public static void main(String[] args) {
      int a = 56776;
      int n1 = a/10000;
      int n2 = (a/1000)%10;
      int n3 = (a/100)%10;
      int n4 = (a/10)%10;
      int n5 = a % 10;
      
      if (n1 == n5 && n2 == n4) {
         System.out.println("True");
      }
      else {
           System.out.println("False");
      }
  }
}



Задание 2:

public class Main
{
  public static void main(String[] args) {
      int a = 14345;
      int n1 = a/10000;
      int n2 = (a/1000)%10;
      int n3 = (a/100)%10;
      int n4 = (a/10)%10;
      int n5 = a % 10;
      
      if (n1 == n2  n1 == n3  n1 == n4  n1 == n5  n2 == n3  n2 == n4  n2 == n5  n3 == n4  n3 == n5  n4 == n5) {
              System.out.println("Есть одинаковые цифры");
      }
      else {
           System.out.println("Нет одинаковых цифр");
      }
  }
}



Задание 3:

public class Main
{
  public static void main(String[] args) {
      int a = 3303;
      int n1 = a/1000;
      int n2 = (a/100)%10;
      int n3 = (a/10)%10;
      int n4 = a%10;
    
      
      if (n1 == n2 && n2 == n3  n1 == n3 && n3 == n4|| n1 == n4 && n4 == n2  n2 == n3 && n3 == n4) {
            
              System.out.println("Есть 3 одинаковые цифры");
      }
      else {
           System.out.println("Нет 3 одинаковых цифр");
      }
  }
}



Задание 4:

public class Main
{
  public static void main(String[] args) {
     int a = 1231;
      int n1 = a/1000;
      int n2 = (a/100)%10;
      int n3 = (a/10)%10;
      int n4 = a%10;
    
      
      if (n1 == n2  n1 == n3  n1 == n4  n2 == n3  n2 == n4  n3 == n4) {
              System.out.println("False");
      }
      else {
           System.out.println("True");
      }
  }
}



